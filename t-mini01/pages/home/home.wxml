<view>

    <!--    主题theme的画面-->
    <image class="themeA" src="{{themeA.entrance_img}}"></image>

    <!--    Banner的画面-->
    <swiper class="swiper"
            indicator-dots
            indicator-active-color="#157658"
            autoplay
            circular>
        <block wx:for="{{bannerB.items}}">
            <swiper-item>
                <image class="swiper" src="{{item.img}}"></image>
            </swiper-item>
        </block>
    </swiper>

    <!--    六宫格的画面-->
    <s-category-grid grid="{{grid}}"></s-category-grid>

    <!--    优惠券-->
    <image class="activity" src="{{activityD.entrance_img}}"></image>

    <s-spu-scroll
            theme="{{themeE}}"
            spu-list="{{themeESpu}}"
            wx:if="{{themeE.online}}">
    </s-spu-scroll>
</view>
